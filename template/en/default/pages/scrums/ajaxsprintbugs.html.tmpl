[% # json_text %]
{ "errormsg" : "[% errors %]", "data" :
{"team_id": "[% sprint.team_id() %]", "is_current": "[% sprint.is_current() %]", "id": "[% sprint.id() %]", "_status": "[% sprint.status() %]", "end_date": "[% sprint.end_date() %]", "start_date": "[% sprint.start_date() %]", "name": "[% sprint.name() %]", "description": "[% sprint.description() %]", "nominal_schedule": "[% sprint.nominal_schedule() %]","bugs": [[% FOREACH bug IN sprint.get_bugs() %] [% bugid = bug.0 %] [[[% bug.0 %],"[% "${bugid}" FILTER bug_link(bugid) FILTER none | replace( '"', '\'' ) %]"], [% bug.1 %], "[% bug.2 %]", "[% bug.3 %]", "[% bug.4 | replace( '"', '\"' ) %][% IF (bug.5.length > 40) %]...[% END %]", "[% bug.5 | replace( '"', '\'' ) %]"][%- "," IF NOT loop.last %][% END %]], "estimatedcapacity" : "[% estimatedcapacity %]" , "personcapacity" : "[% personcapacity %]" , "prediction" : "[% prediction %]", "history" : "[% IF history %]<table>[% FOREACH sprint IN history %]<tr><td>[% sprint.0 %]: </td><td style='min-width: 110px;'>Total work [% sprint.1 %]</td><td>Total persons [% sprint.2 %]</td></tr>[% END %]</table>[% END %]"} }

